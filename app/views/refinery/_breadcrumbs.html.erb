<div class="container">
<h1 class="pull-left"><%= @page.title %></h1>
<ul class="pull-right breadcrumb">
  <% if home_page %>
      <li><a href="/"><%= home_page.title %></a></li>
  <% end %>

  <% @page.ancestors.each do |p| %>
      <li><%= link_to p.title, p.url %></li>
  <% end %>
  <li class="active"><%= @page.title %></li>
</ul>
</div>

<div class="row">
  <div class="col-md-9">
    <% if part = @page.part_with_title(:body) %>
        <% presenter = Refinery::Pages::PagePartSectionPresenter.new(part) %>
        <%= presenter.wrapped_html %>
    <% end %>
  </div>
  <div class="col-md-3">
    <% if part = @page.part_with_title(:right_column) %>
        <% presenter = Refinery::Pages::PagePartSectionPresenter.new(part) %>
        <%= presenter.wrapped_html %>
    <% end %>
  </div>
</div>

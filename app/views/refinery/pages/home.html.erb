<%= content_for :header do %>
    <div class="header margin-bottom-20">
      <%= render "/refinery/header" %>
    </div>
<% end %>

<div class="container">
    <div class="row">
      <div class="col-md-9">
        <%= raw @page.content_for(:slideshow) %>
      </div>
      <div class="col-md-3">
        <%= raw @page.content_for(:right_column) %>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <%= raw @page.content_for(:body) %>
        <%= render "/refinery/photo_gallery/albums/show" %>
      </div>
    </div>
</div>
